.header {
    display: flex;
    width: 100%;
    justify-content: space-between;
    flex-flow: row nowrap;
    align-items: center;
    margin-top: 50px;
    position: relative;
    z-index: 200;
}
.header-logo {
    font-size: 36px;
    font-weight: 800;
}
.header-menu {
    display: flex;
}

.header-soc {
    display: flex;
    @include tablets {
        position: absolute;
        right: 95px;
    }
    @include phones {
        display: none;
    }
}
