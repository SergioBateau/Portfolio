.skills {
    display: flex;
    width: 100%;
}
.skills__container {
    display: flex;
    height: 100vh;
    flex-direction: column;
}
.skills__content {
    display: flex;
    flex-flow: column nowrap;
    align-items: flex-start;
    justify-content: center;
    margin-top: 150px;
    padding: 0 26% 0 34%
}

.skills__content__text {
    margin-top: 70px;
    font-weight: 600;
    font-size: 16px;
    color: white;
    opacity: .5;
}

